{
  "name": "Sonores",
  "version": "0.0.1",
  "description": "An extension to facilitate net neutrality",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "http://127.0.0.1:8080/"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "ChromiePop"
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2,

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "all_frames": true,
      "js": ["libraries/jquery/jquery.min.js", "content.js"]
    }
  ]
}
